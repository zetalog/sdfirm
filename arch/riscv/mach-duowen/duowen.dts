		interrupts = <84>;
		interrupt-parent = <&plic0>;
		clock-frequency = <APB_CLK_FREQ>;
		reg = <0xff 0x63000000 0x0 0x100>;
		reg-io-width = <4>;
		reg-shift = <2>;
		compatible = "snps,dw-apb-uart";
